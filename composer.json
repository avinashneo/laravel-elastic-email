{
	"name": "chocoholics/laravel-elastic-email",
	"description": "A Laravel wrapper for Elastic Email",
	"keywords": ["laravel", "email", "elastic email"],
	"license": "MIT",
	"authors": [
		{
			"company": "Artisan Pixelworks",
			"email": "obura@tongoi.com",
			"url": "https://www.tongoi.com",
			"role": "Developer"
		}
	],
	"repositories": [
	    {
		"type": "package",
		"package": {
		    "name": "weaverryan/derp-dangerzone",
		    "version": "dev-master",
		    "source": {
			"url": "git://github.com/weaverryan/derp-dangerzone.git",
			"type": "git",
			"reference": "master"
		    },
		    "autoload": {
			"psr-0" : {
			    "Weaverryan\\DangerZone" : "src"
			}
		    }
		}
	    }
	],
	"support": {
		"email": "obura@tongoi.com"
	},
	"require": {
		"laravel/framework": "5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.6.x",
		"guzzlehttp/guzzle": ">=6.3"
	},
	"autoload": {
		"psr-4": {
			"Chocoholics\\LaravelElasticEmail\\": "src/"
		}
	},
	"extra": {
	    "laravel": {
		"providers": [
		    "Chocoholics\\LaravelElasticEmail\\MailServiceProvider"
		]
	    }
	},
	"minimum-stability": "stable"
}
